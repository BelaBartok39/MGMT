<main class="container">
  <%= link_to sanitize("&larr; Back to Staffing"), staff_dates_path(@staff_date) %>

  <div class="header">
    <h1>New item for <%= l(@staff_date.date, format: :long) %></h1>
  </div>

  <%= turbo_frame_tag nested_dom_id(@staff_date, Employee.new) do %>
  <%= render "form",
            staff_date: @staff_date,
            employee: @employee %>
  <% end %>
</main>